{
  "manifest_version": 2,

  "name": "Base64 Encoder/Decoder Modified",
  "short_name": "Base64",
  "description": "Encodes and decodes media to and from Base64.",
  "version": "1.2.1",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16":  "icon16.png",
    "48":  "icon48.png",
    "128":  "icon128.png"
  },
  "browser_action": {
   "default_popup": "popup.html",
   "default_title": "Convert to and from Base 64"
  }
}
